# ==========================================
# LLM Configuration
# ==========================================
# Supported providers: groq, gemini, openai, vllm
LLM_PROVIDER=your_llm_provider_name_here
# Example: llama-3.3-70b-versatile, gemini-2.5-flash
LLM_MODEL_NAME=your_llm_model_name_here

# ==========================================
# API Keys (LLMs & External Services)
# ==========================================
GROQ_API_KEY=your_groq_api_key_here
# GROK_API_KEY=your_grok_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
MEITY_API_KEY_VALUE=your_bhashini_api_key

# ==========================================
# Inference / Custom Endpoints
# ==========================================
INFERENCE_ENDPOINT_URL=your_inference_endpoint_url_here
INFERENCE_API_KEY=your_inference_api_key_here

# ==========================================
# Geography / Maps Services
# ==========================================
MAPBOX_API_TOKEN=your_mapbox_token_here

# ==========================================
# Marqo Database
# ==========================================
MARQO_ENDPOINT_URL=http://localhost:8882
MARQO_INDEX_NAME=oan_index

# ==========================================
# Beckn / ONDC Configuration
# ==========================================
BAP_ID=your_bap_id
BAP_URI=your_bap_uri
BAP_ENDPOINT=your_api_endpoint